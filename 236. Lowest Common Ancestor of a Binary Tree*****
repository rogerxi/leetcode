class Solution(object):
    def lowestCommonAncestor(self, root, p, q):
        """
        :type root: TreeNode
        :type p: TreeNode
        :type q: TreeNode
        :rtype: TreeNode
        """
#         # Get the paths, then compare them.
#         # Time complexity:  O(n).   n: number of nodes.
#         # Space complexity: O(n).   n: number of nodes.
